<Window x:Class="MidiToSerial2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Midi To Serial 1.01" Height="350" Width="525" KeyDown="MainWindow_OnKeyDown" KeyUp="MainWindow_OnKeyUp" Closed="MainWindow_OnClosed">
    <Grid>
        <ListView ItemsSource="{Binding}" BorderThickness="0">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <TextBlock Text="{Binding}"/>
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
    </Grid>
</Window>
